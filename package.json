{
  "name": "webpack_boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll=false",
    "dev:renderer": "webpack-dev-server --mode development --env.api=local --config ./config/webpack.renderer.dev.js",
    "dev:main": "webpack --mode development --env.api=local --config ./config/webpack.main.dev.js",
    "dev": "npm run dev:renderer & npm run dev:main",
    "electron:dev": "electron dev/main.js",
    "electron:prod": "electron dist/main.js",
    "prod": "rm -rf ./dist/* && webpack --mode production --env.api=prod --display-error-details --config ./config/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/R3tter/webpack_boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/R3tter/webpack_boilerplate/issues"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "transform": {
      "^.+\\.(jsx|js)$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "homepage": "https://gitlab.com/R3tter/webpack_boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@svgr/webpack": "^5.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "mini-css-extract-plugin": "^1.3.4",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@jmind.systems/react-modal": "0.0.27",
    "@jmind.systems/react-status-controller": "0.0.14",
    "aphrodite": "^2.4.0",
    "api-req-redux": "2.0.10",
    "core-js": "^3.8.3",
    "electron": "^12.0.2",
    "electron-reload": "^1.5.0",
    "i18next": "^19.8.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.5",
    "react-notify-library": "0.0.14",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "reset-css": "^5.0.1",
    "use-form-validation-hook": "^1.0.19"
  }
}
